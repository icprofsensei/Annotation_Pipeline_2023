                                "offset": 1264
                            }
                        },
                        {
                            "text": "bacteria",
                            "infons": {
                                "identifier": "NCBI:txid2",
                                "type": "bacteria_superkingdom",
                                "annotator": "dhylan.patel21@imperial.ac.uk",
                                "date": "2023-10-16 21:38:06",
                                "parent_taxonomic_id": "noParentIDinList"
                            },
                            "id": "463949",
